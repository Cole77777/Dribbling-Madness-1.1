{
  "expo": {
    "name": "Dribbling Madness",
    "slug": "dribbling-madness",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "dribblingmadness",
    "primaryColor": "#FF8C00",
    "description": "Basketball trivia at milestones, facts every minute. Tap to earn points, unlock balls, daily challenges, rebirth. No account, no ads.",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dribblingmadness.app",
      "buildNumber": "15",
      "config": {
        "usesNonExemptEncryption": false
      },
      "icon": "./assets/icon.png",
      "backgroundColor": "#0A0A0F",
      "infoPlist": {
        "UIRequiresFullScreen": true,
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "package": "com.dribblingmadness.app",
      "versionCode": 15
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "basePath": "/Dribbling-Madness-1.1/"
      }
    },
    "plugins": [
      "expo-asset",
      "./plugins/withIosAppIcon.js"
    ],
    "extra": {
      "eas": {
        "projectId": "11a90041-7660-416e-bdae-acec8361d186"
      }
    }
  }
}